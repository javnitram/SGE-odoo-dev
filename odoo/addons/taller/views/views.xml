<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="taller.vehiculos">
      <field name="name">taller.list</field>
      <field name="model">ig.taller.vehiculos</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="matricula"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="taller.action_window1">
      <field name="name">Vehiculos</field>
      <field name="res_model">ig.taller.vehiculos</field>
      <field name="view_mode">tree,form</field>
    </record>

    <!-- server action to the one above -->
<!--
    <record model="ir.actions.server" id="taller.action_server">
      <field name="name">taller server</field>
      <field name="model_id" ref="model_taller_taller"/>
      <field name="state">code</field>
      <field name="code">
        action = {
          "type": "ir.actions.act_window",
          "view_mode": "tree,form",
          "res_model": model._name,
        }
      </field>
    </record>
-->

    <!-- Top menu item -->

    <menuitem name="Taller" id="taller.menu_root"/>
    <!-- menu categories -->

    <menuitem name="Vehiculos" id="taller.menu_1" parent="taller.menu_root" action="taller.action_window1"/>
  

    <!-- actions-->

    
  </data>
