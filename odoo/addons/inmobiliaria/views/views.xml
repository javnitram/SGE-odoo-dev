    <!-- Top menu item -->
    <!--               -->

    <menuitem name="Inmobiliaria" id="il.inmobiliaria.menu_root"/>

    <!-- menu categories -->
    <!--                 -->

    <menuitem name="Inmueble" id="il.inmobiliaria.inmueble" parent="il.inmobiliaria.menu_root"/>
    <!--<menuitem name="Venta" id="il.inmobiliaria.menu_2" parent="il.inmobiliaria.menu_root"/> -->

    <!-- actions -->
