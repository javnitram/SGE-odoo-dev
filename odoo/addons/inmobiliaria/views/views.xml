<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="il.inmobiliaria.inmueble.list">
      <field name="name">Lista de inmuebles</field>
      <field name="model">il.inmobiliaria.inmueble</field>
      <field name="arch" type="xml">
        <tree>
          <field name="name"/>
          <field name="value"/>
          <field name="value2"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="il.inmobiliaria.inmueble" id="il.inmobiliaria.inmueble.action_window">
      <field name="name">Inmueble window</field>
      <field name="res_model">il.inmobiliaria.inmueble</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
