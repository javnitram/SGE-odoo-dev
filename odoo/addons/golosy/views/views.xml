<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="golosy_golosinas_tree">
      <field name="name">golosy.golosinas.tree</field>
      <field name="model">wb.golosy.golosinas</field>
      <field name="arch" type="xml">
        <tree>
          <field name="id_asig"/>
          <field name="name"/>
          <field name="precio"/>
          <field name="description"/>
        </tree>
      </field>
    </record>
