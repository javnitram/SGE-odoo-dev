<?xml version="1.0" encoding="utf-8"?>
<odoo>

<data>

    <record model="ir.ui.view" id="concesionario.empleados_tree">
      <field name="name">Empleados.tree</field>
      <field name="model">concesionario.empleados</field>
      <field name="arch" type="xml">
        <tree>
          <field name="nombre"></field>
          <field name="apellidos"></field>
          <field name="nrovendidos"></field>
          <field name="salario"></field>
        </tree>
      </field>
    </record>
<record id="concesionario.empleados_kanban" model="ir.ui.view">
  <field name="name">Empleados.kanban</field>
  <field name="model">concesionario.empleados</field>
  <field name="arch" type="xml">
    <kanban>
      <templates>
        <t t-name="kanban-box">
          
        </t>
      </templates>
    </kanban>
  </field>
</record>
    
  <!-- Action windows-->
    
        
  <record model="ir.actions.act_window" id="concesionario.empleados_action_window_tree">
    <field name="name">Empleados</field>
    <field name="res_model">concesionario.empleados</field>
    <field name="view_mode">tree</field>
  </record>

  <record model="ir.actions.act_window" id="concesionario.empleados_action_window_kanban">
    <field name="name">Empleados</field>
    <field name="res_model">concesionario.empleados</field>
    <field name="view_mode">kanban</field>
  </record>
</data>


</odoo>
