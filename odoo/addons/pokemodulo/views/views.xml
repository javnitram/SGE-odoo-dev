    <!-- Top menu item -->

    <menuitem name="Pokemodulo" id="pokemodulo.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Lista Equipos" id="pokemodulo.equipo" parent="pokemodulo.menu_root"/>
    <menuitem name="Lista Entrenadores" id="pokemodulo.entrenadoresL" parent="pokemodulo.menu_root" action="pokemodulo.entrenadores_action_window"/>
    <!-- actions -->

    <menuitem name="Equipo1" id="pokemodulo.menu_1_list" parent="pokemodulo.equipo"
              action="pokemodulo.equipo1_action_window"/>

    <menuitem name="Equipo2" id="pokemodulo.menu_2_list" parent="pokemodulo.equipo"
              action="pokemodulo.equipo2_action_window"/>

    <menuitem name="Equipo3" id="pokemodulo.menu_3_list" parent="pokemodulo.equipo"
              action="pokemodulo.equipo3_action_window"/>



