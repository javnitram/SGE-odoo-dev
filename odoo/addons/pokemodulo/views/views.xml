<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="pokemodulo.list">
      <field name="name">Pokemodulo Equipo</field>
      <field name="model">ar.pokemodulo.equipo1</field>
      <field name="arch" type="xml">
        <tree>
          <field name="Imagen" widget="image" class="oe_right oe_avatar" options="{'size':[265,300]}"  />
          <field name="Nombre"/>
          <field name="Mote"/>
          <field name="Tipo1"/>
          <field name="Tipo2" />
        </tree>
      </field>
    </record>


    <record model="ir.ui.view" id="pokemodulo.list2">
      <field name="name">Pokemodulo Equipo</field>
      <field name="model">ar.pokemodulo.equipo2</field>
      <field name="arch" type="xml">
        <tree>
          <field name="Imagen" widget="image" class="oe_right oe_avatar" options="{'size':[265,300]}"  />
          <field name="Nombre"/>
          <field name="Mote"/>
          <field name="Tipo1"/>
          <field name="Tipo2" />
        </tree>
      </field>
    </record>


    <record model="ir.ui.view" id="pokemodulo.list3">
      <field name="name">Pokemodulo Equipo</field>
      <field name="model">ar.pokemodulo.equipo3</field>
      <field name="arch" type="xml">
        <tree>
          <field name="Imagen" widget="image" class="oe_right oe_avatar" options="{'size':[265,300]}"  />
          <field name="Nombre"/>
          <field name="Mote"/>
          <field name="Tipo1"/>
          <field name="Tipo2" />
        </tree>
      </field>
    </record>



    <record model="ir.ui.view" id="pokemodulo.entrenadores">
      <field name="name">Pokemodulo Entrenadores</field>
      <field name="model">ar.pokemodulo.entrenadores</field>
      <field name="arch" type="xml">
        <tree>
          <field name="ImagenE" widget="image" class="oe_right oe_avatar" options="{'size':[265,300]}"  />
          <field name="Nombre"/>
          <field name="Equipo"/>
        </tree>
      </field>
    </record>



    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="pokemodulo.equipo1_action_window">
      <field name="name">Pokemodulo Equipo1</field>
      <field name="res_model">ar.pokemodulo.equipo1</field>
      <field name="view_mode">tree,form</field>
    </record>

    <record model="ir.actions.act_window" id="pokemodulo.equipo2_action_window">
      <field name="name">Pokemodulo Equipo2</field>
      <field name="res_model">ar.pokemodulo.equipo2</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="pokemodulo.equipo3_action_window">
      <field name="name">Pokemodulo Equipo3</field>
      <field name="res_model">ar.pokemodulo.equipo3</field>
      <field name="view_mode">tree,form</field>
    </record>


    <record model="ir.actions.act_window" id="pokemodulo.entrenadores_action_window">
      <field name="name">Pokemodulo Entrenadores</field>
      <field name="res_model">ar.pokemodulo.entrenadores</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
