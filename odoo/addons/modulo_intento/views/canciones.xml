<odoo>
    <data>
      <!-- explicit list view definition -->
  
      <record model="ir.ui.view" id="yf_cancion_list">
        <field name="name">disco.cancion</field>
        <field name="model">yf.disco.cancion</field>
        <field name="arch" type="xml">
          <tree>
            <field name="nombre"/>
            <field name="duracion"/>
            <field name="album"/>
            <field name="artista"/>
            <field name="genero"/>
          </tree>
        </field>
      </record>
  
      <!-- actions opening views on models -->
  
      <record model="ir.actions.act_window" id="modulo_intento.action_window4">
        <field name="name">Canciones</field>
        <field name="res_model">yf.disco.cancion</field>
        <field name="view_mode">tree,form</field>
      </record>
  
      <!-- Top menu item -->
  
      <!-- <menuitem name="Tienda Discos" id="modulo_intento.menu_root"/> -->
  
      <!-- menu categories -->
  
      <menuitem name="Canciones" id="modulo_intento.menu_4" parent="modulo_intento.menu_root"
                action="modulo_intento.action_window4"/>
  
      <!-- actions -->
  
      <!--<menuitem name="List" id="modulo_intento.menu_1_list" parent="modulo_intento.menu_1"
                action="modulo_intento.action_window"/>-->
      <!-- <menuitem name="List" id="modulo_intento.menu_2_list" parent="modulo_intento.menu_2"
                action="modulo_intento.action_window"/> -->
    
  
    </data>
  </odoo>