<odoo>
    <data>
      <!-- explicit list view definition -->
  
      <record model="ir.ui.view" id="yf_discografica_list">
        <field name="name">disco.discografica</field>
        <field name="model">yf.disco.discografica</field>
        <field name="arch" type="xml">
          <tree>
            <field name="name"/>
            <field name="anhofundacion"/>
          </tree>
        </field>
      </record>
      <record model="ir.ui.view" id="yf_discografica_form">
        <field name="name">disco.discografica</field>
        <field name="model">yf.disco.discografica</field>
        <field name="arch" type="xml">
          <form>
            <group colspan="2" col="2">
              <field name="name"/>
              <field name="anhofundacion"/>
          </group>
          </form>
        </field>
      </record>
  
      <!-- actions opening views on models -->
  
      <record model="ir.actions.act_window" id="modulo_intento.action_window10">
        <field name="name">Discograficas</field>
        <field name="res_model">yf.disco.discografica</field>
        <field name="view_mode">tree,form</field>
      </record>
      <record model="ir.actions.act_window" id="modulo_intento.action_window11">
        <field name="name">Discograficas</field>
        <field name="res_model">yf.disco.discografica</field>
        <field name="view_mode">form</field>
      </record>
  
      <!-- Top menu item -->
  
      <!-- <menuitem name="Tienda Discos" id="modulo_intento.menu_root"/> -->
  
      <!-- menu categories -->
  
      <menuitem name="Discograficas" id="modulo_intento.menu_10" parent="modulo_intento.menu_root"/>
  
      <!-- actions -->
  
      <menuitem name="Tree" id="modulo_intento.menu_10_list" parent="modulo_intento.menu_10"
                action="modulo_intento.action_window10"/>
      <menuitem name="Form" id="modulo_intento.menu_11_list" parent="modulo_intento.menu_10"
                action="modulo_intento.action_window11"/>
