<odoo>
  <data>
    <!-- explicit list view definition -->

    <record model="ir.ui.view" id="yf_genero_list">
      <field name="name">categoria.disco</field>
      <field name="model">yf.discos.genero</field>
      <field name="arch" type="xml">
        <tree>
          <field name="nomcat"/>
        </tree>
      </field>
    </record>


    <!-- actions opening views on models -->

    <record model="ir.actions.act_window" id="modulo_intento.action_window">
      <field name="name">Genero</field>
      <field name="res_model">yf.discos.genero</field>
      <field name="view_mode">tree,form</field>
    </record>


    <!-- server action to the one above -->
