    <!-- Top menu item -->

    <menuitem name="Discos" id="modulo_intento.menu_root"/>

    <!-- menu categories -->

    <menuitem name="Menu 1" id="modulo_intento.menu_1" parent="modulo_intento.menu_root"/>
    <menuitem name="Menu 2" id="modulo_intento.menu_2" parent="modulo_intento.menu_root"/>

    <!-- actions -->

